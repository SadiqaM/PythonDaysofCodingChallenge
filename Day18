# Create a program to find the largest among three numbers.

def find_max1(input_list):
    return max(input_list)

print(find_max1([10,-45,199]))
print(find_max1([10,10,10]))

def find_max(num1, num2, num3):
    if num1 > num2 :
        max_num = num1
    else:
        max_num = num2

    if max_num > num3:
        return max_num
    else:
        return num3

print(find_max(10, 10, 10))
print(find_max(190, -10, 999))
